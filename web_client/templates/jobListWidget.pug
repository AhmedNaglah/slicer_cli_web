table.g-jobs-list-table
  tbody
    each job in jobs
      - var title = job.title.replace(/^HistomicsTK\./, '');
      tr(data-job-id=job._id)
        td.g-status-icon-container(status=job.status)
          i(class=JobStatus.icon(job.status))
        td
          a(href=`/#job/${job._id}`, target='_blank')= title
        if job.paramFile
          td
            a.s-param-file(data-file-id=job.paramFile)
              i.icon-link-ext-alt
