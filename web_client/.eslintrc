{
  "extends": "semistandard",
  "root": true,
  "globals": {
    "$": true
  },
  rules: {
    "indent": [2, 4, {"SwitchCase": 1}],
    "space-before-function-paren": [2, {"anonymous": "always", "named": "never"}],
    "one-var": 0,
    "no-multi-spaces": [2, {"exceptions": {"VariableDeclarator": true, "ImportDeclaration": true} }]
  },
  "env": {
    "browser": true
  }
}
